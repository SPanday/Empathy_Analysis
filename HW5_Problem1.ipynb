    "print('     testing on REFINED validation set...') \n",
    "predicted = clf.predict(X_val)\n",
    "print('       accuracy on validation set:',np.mean(predicted == y_val))\n",
    "accuracies.append(np.mean(predicted == y_val))"
   ]
